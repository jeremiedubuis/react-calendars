  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.6"
